                  const progressBar = document.querySelector(".progressBar");

                  const uploadImage = () => {
                    loading.style.display = "block";
                    const storageRef = storage.ref().child("myimages");
                    const folderRef = storageRef.child(fileName);
                    const uploadtask = folderRef.put(file);
                    uploadtask.on(
                      "state_changed",
                      (snapshot) => {
                        console.log("Snapshot", snapshot.ref.name);
                        thanhngang = (snapshot.bytesTransferred / snapshot.totalBytes) * 100;
                        thanhngang = Math.round(thanhngang);
                        progressBar.style.display = "block";
                        progressbar.style.width = thanhngang + "%";
                        progressbar.innerHTML = thanhngang + "%";
                        uploadedFileName = snapshot.ref.name;
                      },
                      (error) => {
                        console.log(error);
                        progressBar.style.display = "none";
                      },
                      () => {
                        storage
                          .ref("myimages")
                          .child(uploadedFileName)
                          .getDownloadURL()
                          .then((url) => {
                            console.log("URL", url);
                            if (!url) {
                              img.style.display = "none";
                            } else {
                              img.style.display = "block";
                              loading.style.display = "none";
                            }
                            img.setAttribute("src", url);
                            productImageInput.value = url;
                            fa_times.style.display = "block";
                            fileName = ""; // Đặt lại giá trị của fileName
                            fileData.innerHTML = ""; // Đặt fileData thành chuỗi rỗng
                            inp.value = "";
                          });
                          console.log(productImageInput);
                        console.log("File Uploaded Successfully");
                      }
                    );
                  };
                  const deleteImage = () => {
                    if (uploadedFileName) {
                      const storageRef = storage.ref().child("myimages");
                      const imageRef = storageRef.child(uploadedFileName);
                      imageRef
                        .delete()
                        .then(() => {
                          console.log("File deleted successfully");
                          img.style.display = "none";
                          fileData.style.display = "none";
                          progressBar.style.display = "none";
                          progressbar.style.width = "0%";
                          progressbar.innerHTML = "0%";
                          uploadedFileName = null;
                          fa_times.style.display = "none";
                        })
                        .catch((error) => {
                          console.log("Error deleting file:", error);
                        });
                    } else {
                      console.log("No file to delete.");
                    }
                  };
